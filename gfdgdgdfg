        local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))() 
local PhantomForcesWindow = Library:NewWindow("Pvp Games Booga")
 
local KillingCheats = PhantomForcesWindow:NewSection("Pvp Game 8")
 

 
KillingCheats:CreateButton("Auto Heal (Bloodfruit) F1", function()
    game:GetService("UserInputService").InputBegan:Connect(function(input)
        if input.KeyCode == Enum.KeyCode.F1 then
          if _G.enabled == true then
            _G.enabled = false
          else
            _G.enabled = true
          end
        end
      end)
      
      while wait() do
        if _G.enabled == true then
              -- Script generated by SimpleSpy - credits to exx#9394
        
        local args = {
            [1] = "Bloodfruit"
        }
        
        game:GetService("ReplicatedStorage").Events.LolAutoHealers:FireServer(unpack(args))
        
        
        
          -- Script generated by SimpleSpy - credits to exx#9394
        
        local args = {
            [1] = "Bloodfruit"
        }
        
        game:GetService("ReplicatedStorage").Events.LolAutoHealers:FireServer(unpack(args))
        
        
        
        
          -- Script generated by SimpleSpy - credits to exx#9394
        
        local args = {
            [1] = "Bloodfruit"
        }
        
        game:GetService("ReplicatedStorage").Events.LolAutoHealers:FireServer(unpack(args))
        
        
        end
        end
    end)
     


     
